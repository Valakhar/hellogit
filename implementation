#include "Game.h"
#include <iostream>
#include <string>
using namespace std;
void goodgame (string n)
{
if (n="LeagueofLegends")
{
cout <<"Good Game";
name = n;
}
else 
{
cout<<"Bad Game";
}
void setname (string n)
{
name = n;
}
string getname()
{
return name;
}
void setplaytime (float p)
{
playtime = p;
}
float getplaytime ()
{
return playtime;
}
void displayInfo ()
{ 
cout <<"Game Name "<< name<< endl;
cout << "Time Played"<< playtime<< endl;
}
}
